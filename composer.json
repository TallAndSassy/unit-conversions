{
    "name": "tallandsassy/unit-conversions",
    "description": "Simple converts",
    "keywords": [
        "tallandsassy",
        "unit-conversions"
    ],
    "homepage": "https://github.com/tallandsassy/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "JJ Rohrer",
            "email": "jj.github@rohrer.org",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Tallandsassy\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tallandsassy\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
